<Window x:Class="VodAdsBlocker.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VodAdsBlocker"
        mc:Ignorable="d"
        Background="#2D2D30"
        Title="VodAdsBlocker - Ustawienia" Height="120" Width="300" ResizeMode="NoResize">
    <Grid Margin="10">
        <CheckBox Foreground="#ddd" x:Name="cbAutostart" Checked="ToggleButton_OnChecked" Unchecked="ToggleButton_OnUnchecked">Uruchamiaj wraz ze startem systemu</CheckBox>
    </Grid>
</Window>
